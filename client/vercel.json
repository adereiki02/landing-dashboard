{
  "env": {
    "REACT_APP_API_URL": "https://reikidevs-official-production.up.railway.app",
    "REACT_APP_USE_ABSOLUTE_MEDIA_URLS": "true",
    "REACT_APP_NEWS_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads/news",
    "REACT_APP_PORTFOLIO_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads/portfolio",
    "REACT_APP_TESTIMONIAL_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads/testimonials",
    "REACT_APP_PARTNER_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads/partners",
    "REACT_APP_PROFILE_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads/profiles",
    "REACT_APP_DEFAULT_IMAGES_URL": "https://reikidevs-official-production.up.railway.app/uploads"
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}